<div class="page-header">
  <h2>Victory List</h2>
</div>
<div id="container">
  <h3>My Victories</h3>
  <table>
    <tr>
      <th>Vitory ID</th>
      <th>Title</th>
    </tr>
    <% @myvictory.each do |myvictory| %>
        <tr>
          <td><%= myvictory.id %></td>
          <td><%= link_to myvictory.title, "/victories/#{myvictory.id}" %></td>
        </tr>
    <% end %>
  </table>
</div>
<br/>
<br/>
<br/>
<h3>ALL Victories</h3>
<table>
  <tr>
    <th>Vitory ID</th>
    <th>Title</th>
    <th>Posted By</th>
  </tr>
  <% @victory.each do |victory| %>
      <tr>
        <td><%= victory.id %></td>
        <td><%= link_to victory.title, "/victories/#{victory.id}" %></td>
        <td><%= Profile.find_by_user_id(victory.user_id).first_name %></td>
      </tr>
  <% end %>
</table>

<%= will_paginate @victory %>